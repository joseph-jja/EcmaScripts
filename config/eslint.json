{
    "ecmaFeatures": {
        "modules": true
    },

    "parserOptions": "ecmaVersion": 7,
    "sourceType": {
        "module": {
            "modules": true,
            "ecmaFeatures": {
                "modules": true
            }
        }
    },
    "env": {
        "browser": true,
        "node": true,
        "es6": 1,
    },

    "rules": {
        "no-bitwise": 1,
        "camelcase": 1,
        "curly": 1,
        "eqeqeq": 2,
        "guard-for-in": 0,
        "no-extend-native": 0,
        "wrap-iife": 1,
        "indent": 2,
        "no-undef": 2,
        "new-cap": 1,
        "no-caller": 1,
        "no-empty": 1,
        "no-irregular-whitespace": 1,
        "no-new": 1,
        "no-plusplus": 0,
        "no-unused-vars": 1,
        "strict": 0,
        "comma-dangle": 1,
        "no-trailing-spaces": 0,
        "max-params": 1,
        "max-depth": 1,
        "max-statements": 1,
        "complexity": 1,
        "max-len": 0,

        "semi": 1,
        "no-cond-assign": 0,
        "no-debugger": 0,
        "no-eq-null": 1,
        "no-eval": 1,
        "quotes": 0,
        "no-underscore-dangle": 0,
        "dot-notation": 0,

        "space-unary-ops": 0
    },

    "globals": {
        "jQuery": 1,
        "require": 1,
        "define": 1,
        "document": 1,
        "window": 1,
        "console": 1,
        "JSON": 1,
        "google": 1,
        "setInterval": 1,
        "clearInterval": 1,

        "describe": 1,
        "it": 1,
        "xit": 1,
        "expect": 1,
        "beforeEach": 1,
        "afterEach": 1,
        "beforeAll": 1,
        "afterAll": 1,
        "spyOn": 1,
        "loadJSONFixtures": 1,
        "loadFixtures": 1,
        "setFixtures": 1,
        "getJSONFixture": 1,
        "jasmine": 1,
        "loadStyleFixtures": 1,
        "preCondition": 1,
        "affix": 1,

        "escape": 1,

        "assetsServer": 1,
        "commonAssetsServer": 1,
        "isYAQF": 1,
        "Modernizr": 1,
        "pdpAssetServer": 1,
        "secureServer": 1,
        "ssp": 1,
        "unsecureServer": 1
    }

}
