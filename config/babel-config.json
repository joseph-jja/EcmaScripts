{
    "presets": [
        [ "@babel/preset-env", {
            "targets": {
                "ie": "11",
                "edge": "15",
                "chrome": "60",
                "firefox": "60",
                "safari": "10",
                "esmodules": false
            }
        } ]
    ],
    "plugins": [
        [ "@babel/plugin-transform-runtime",
            {
                "corejs": false,
                "helpers": true,
                "regenerator": true,
                "useESModules": false
            }
        ]
    ],
    "sourceMaps": "both"
}
