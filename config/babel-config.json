{
    "presets": [
        [ "@babel/preset-env", {
            "targets": {
                "edge": "100",
                "chrome": "100",
                "firefox": "100",
                "safari": "15"
            }
        } ]
    ],
    "plugins": [
        [ "@babel/plugin-transform-runtime",
            {
                "corejs": false,
                "helpers": true,
                "regenerator": true,
                "useESModules": false
            }
        ]
    ],
    "sourceMaps": "both"
}
